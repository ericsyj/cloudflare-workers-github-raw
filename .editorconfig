# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
end_of_line = lf
quote_type = double
curly_bracket_next_line = false
spaces_around_operators = true
trim_trailing_whitespace = false
insert_final_newline = false

[*.js]
indent_size = 4
quote_type = single
