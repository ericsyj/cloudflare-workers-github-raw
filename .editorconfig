# http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
tab_width = 2
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[*.js]
tab_width = 4
quote_type = single
curly_bracket_next_line = false
spaces_around_operators = true
indent_brace_style = 1tbs
